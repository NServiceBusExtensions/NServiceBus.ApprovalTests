<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <Version>1.12.0</Version>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright $([System.DateTime]::UtcNow.ToString(yyyy)). All rights reserved.</Copyright>
    <PackageTags>NServiceBus, ApprovalTests</PackageTags>
    <Description>Adds ApprovalTests (https://github.com/approvals/ApprovalTests.Net) support to NServiceBus (https://docs.particular.net/nservicebus/).</Description>
    <PackageIconUrl>https://raw.githubusercontent.com/NServiceBusExtensions/NServiceBus.ApprovalTests/master/src/icon.png</PackageIconUrl>
    <PackageProjectUrl>https://docs.particular.net/nuget/NServiceBus.ApprovalTests</PackageProjectUrl>
    <RepositoryUrl>https://github.com/NServiceBusExtensions/NServiceBus.ApprovalTests</RepositoryUrl>
    <PackageOutputPath>..\..\nugets</PackageOutputPath>
    <LangVersion>latest</LangVersion>
    <NoWarn>CS1591;NU5105</NoWarn>
    <AssemblyOriginatorKeyFile>$(SolutionDir)key.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
  </PropertyGroup>
</Project>